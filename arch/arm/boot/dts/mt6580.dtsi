			interrupts =	<GIC_SPI 93 IRQ_TYPE_LEVEL_LOW>, /* CCIF0 IRQ */
					<GIC_SPI 152 IRQ_TYPE_EDGE_FALLING>;/* MD WDT IRQ */
			mediatek,ccif_capability = <2>;
			mediatek,md_smem_size = <0x200000>; /* md share memory size */
		};

		MCUSYS_CFGREG@0x1020000 {
