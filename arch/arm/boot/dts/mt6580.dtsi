						 <7 4 20>,
						 <8 4 18>,
						 <9 3 20>,
						 <10 3 18>,
						 <24 5 20>,
						 <25 5 18>,
						 <77 2 20>,
						 <78 2 18>,
						 <13 1 17>; /* PMIC */
		};

		ap_ccif0:ap_ccif0@1000C000 {
			compatible = "mediatek,ap_ccif0";
			reg =	<0x1000C000 0x1000>,	/*AP_CCIF_BASE*/
